import java.util.*;
public class avg {
    public static int Greatest(int a,int b){
        return (a>b)?a:b;

    }
    public static void main(String[]args){
            Scanner sc=new Scanner(System.in);
        System.out.println("Enter the number :");
        int a=sc.nextInt();
        int b=sc.nextInt();
        int res=Greatest(a,b);
        System.out.println(res);


    }
}
